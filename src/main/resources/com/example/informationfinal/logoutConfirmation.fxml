<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.palexdev.materialfx.controls.MFXButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane onKeyPressed="#onKeyPressed" prefHeight="125.0" prefWidth="290.0" style="-fx-border-width: 1px; -fx-border-color: lightgray; -fx-border-radius: 6px;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.informationfinal.LogoutConfirmation">
    <children>
        <Text fill="#3c3f51" layoutX="27.0" layoutY="47.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Are you sure you want to Logout?">
            <font>
                <Font name="Roboto" size="16.0" />
            </font>
        </Text>
        <MFXButton fx:id="ok" buttonType="RAISED" layoutX="73.0" layoutY="70.0" onAction="#logout" prefHeight="32.0" prefWidth="68.0" style="-fx-background-color: #3e4754; -fx-text-fill: white;" stylesheets="@css/MFXButton.css" text="OK" />
      <MFXButton fx:id="cancel" buttonType="RAISED" layoutX="149.0" layoutY="70.0" onAction="#close" prefHeight="32.0" prefWidth="68.0" style="-fx-background-color: #3e4754; -fx-text-fill: white;" stylesheets="@css/MFXButton.css" text="Cancel" />
    </children>
</AnchorPane>
